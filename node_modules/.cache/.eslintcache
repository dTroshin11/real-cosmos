:0},"/Users/troshin/dev/Membrana2/src/index.js",[],[],["236"],"/Users/troshin/dev/Membrana2/src/App.js",["237"],[],"/Users/troshin/dev/Membrana2/src/components/HeroBlock/HeroBlock.jsx",["238"],[],"import Header from '../Header/Header';\nimport { useWindowSize } from '../../hooks/useWindowSize';\nimport Styles from './HeroBlock.module.scss';\nimport { useEffect, useState } from 'react';\nimport SoundButton from \"../ui/SoundButton/SoundButton\";\n\nconst HeroBlock = ({ formBlockRef }) => {\n  const { smallMobileWidth, desktopWidth, windowWidth } = useWindowSize();\n  const [srcVideo, setSrcVideo] = useState(null);\n\n  useEffect(() => {\n    // const timestamp = Date.now();\n    // const videoUrl = smallMobileWidth ? `video/hero-video-mobile.mp4?v=${timestamp}` : `video/hero-video.mp4?v=${timestamp}`;\n\n    // console.log(windowWidth);\n\n    if (windowWidth !== null) {\n      if (windowWidth > 440) {\n        setSrcVideo(\n          <source\n            src={\n              'https://storage.yandexcloud.net/cdn.tsekh.design/Membrana/benefits/%5BVer-9-New%5DMTS-Membrana-MainScreen_2880x1600_slow_szh2.mp4#t=0.001'\n            }\n            type='video/mp4'\n          ></source>,\n        );\n      } else {\n        setSrcVideo(\n          <source\n            src={\n              'https://storage.yandexcloud.net/cdn.tsekh.design/Membrana/benefits/%5BVer-9-1_Mob%5DMTS-Membrana-MainScreen_750x1200_slow_szh.mp4#t=0.001'\n            }\n            type='video/mp4'\n          ></source>,\n        );\n      }\n    }\n  }, [windowWidth]);\n\n  return (\n    <div className={Styles.Block}>\n      <Header formBlockRef={formBlockRef} />\n      <video autoPlay='1' playsInline preload={'metadata'} loop muted className={Styles.Video}>\n        {srcVideo}\n      </video>\n      <div className={Styles.Wrapper}>\n          {windowWidth > 1000 ? <p className={Styles.SubTitle}>\n              Уберите лишнее и&nbsp;сфокусируйтесь на&nbsp;важном <br/>с&nbsp;премиум-сервисом Membrana\n          </p> : <p className={Styles.SubTitle}>\n              Уберите лишнее и&nbsp;сфокусируйтесь на&nbsp;важном <br/>с&nbsp;премиум-сервисом Membrana\n          </p> }\n        {smallMobileWidth && (\n          <div className={Styles.Stick}>\n            <div className={Styles.SmallerStick}></div>\n          </div>\n        )}\n      </div>\n        <SoundButton />\n    </div>\n  );\n};\n\nexport default HeroBlock;\n","/Users/troshin/dev/Membrana2/src/components/FaqBlock/FaqBlock.jsx",["239"],[],"import React, { useState } from 'react';\nimport { useWindowSize } from '../../hooks/useWindowSize';\n\nimport Styles from './FagBlock.module.scss';\nimport FagItem from './FaqItem/FagItem';\nimport classNames from 'classnames';\n\nconst BlockContent = () => {\n  const { windowWidth } = useWindowSize();\n  const mobile = windowWidth <= 500;\n  const SDFag = [\n    {\n      title: 'Как подключить Membrana?',\n      content: (\n        <div>\n          <p>Чтобы стать абонентом Membrana, вам нужно:</p>{' '}\n          <ol style={{ marginLeft: '-15px' }}>\n            <li>Получить приглашение со&nbsp;ссылкой на&nbsp;скачивание и&nbsp;реферальным кодом от&nbsp;действующего абонента Membrana</li>\n            <li>Скачать приложение Membrana</li>\n            <li>Активировать реферальный код-приглашение</li>\n          </ol>\n        </div>\n      ),\n    },\n    {\n      title: <span>Какой пакет интернета, минут и&nbsp;SMS входит в&nbsp;Membrana</span>,\n      content: (\n        <div>\n          <ol style={{ listStyle: 'disc', margin: '0', padding: '0 0 0 20px' }}>\n            <li>Безлимитный интернет</li> <li>Безлимитные звонки на&nbsp;МТС</li>{' '}\n            <li>5000 минут на&nbsp;звонки абонентам других операторов</li> <li>1000 SMS</li>\n          </ol>\n        </div>\n      ),\n    },\n    {\n      title: <span>Есть&nbsp;ли раздача интернета?</span>,\n      content: <div>Да, в&nbsp;пакет входит 100&nbsp;ГБ в&nbsp;месяц.</div>,\n    },\n    {\n      title: 'Какая зона покрытия?',\n      content: <div>Сейчас та&nbsp;же, что и&nbsp;у&nbsp;МТС.</div>,\n    },\n    {\n      title: <span>Какие условия в&nbsp;роуминге?</span>,\n      content: (\n        <div>\n          В&nbsp;приложении Membrana вы&nbsp;сможете узнать цены в&nbsp;конкретной стране и&nbsp;заранее рассчитать свои траты\n          в&nbsp;калькуляторе роуминга.\n        </div>\n      ),\n    },\n    {\n      title: <span>Есть&nbsp;ли eSIM? Как его получить?</span>,\n      content: (\n        <div>\n          <span>В&nbsp;приложении Membrana при заказе вы&nbsp;сможете выбрать вариант eSIM.</span>\n          <br /> <span>Как её оформить:</span>\n          <br />{' '}\n          <ol style={{ marginLeft: '-15px' }}>\n            <li>Авторизоваться через Госуслуги</li> <li>Подписать договор электронной подписью в&nbsp;приложении Госключ</li>{' '}\n            <li>Получить QR-код на&nbsp;установку eSIM</li>\n          </ol>\n        </div>\n      ),\n    },\n    {\n      title: <span>Как перейти в&nbsp;Membrana со&nbsp;своим номером?</span>,\n      content: (\n        <div>\n          В&nbsp;приложении Membrana при заказе есть отдельная ветка для перехода со&nbsp;своим номером. Для абонентов МТС это будет просто\n          смена тарифа, а&nbsp;для абонентов других операторов&nbsp;&mdash; смена оператора с&nbsp;сохранением номера.\n        </div>\n      ),\n    },\n    {\n      title: <span>На&nbsp;Membrana могут перейти абоненты других операторов?</span>,\n      content: <div>Да, конечно.</div>,\n    },\n    {\n      title: <span>Приложение Membrana создано для iOS и&nbsp;Android? Где его скачать?</span>,\n      content: <div>Да, для iOS и&nbsp;Android. Приложение будет доступно в&nbsp;Google Play и&nbsp;App Store после запуска.</div>,\n    },\n    {\n      title: <span>Что если мне нужны два номера?</span>,\n      content: (\n        <div>\n          Для заказа второго номера Membrana вы&nbsp;можете воспользоваться одним из&nbsp;5&nbsp;реферальных кодов, выданных вместе\n          с&nbsp;вашим первым номером.\n        </div>\n      ),\n    },\n    {\n      title: 'Как работает маскировка IP?',\n      content: (\n        <div>\n          Вы&nbsp;можете в&nbsp;любой момент изменить IP-адрес без передачи личных данных сторонним сервисам и&nbsp;лишней нагрузки\n          на&nbsp;операционную систему смартфона и&nbsp;батарею.\n        </div>\n      ),\n    },\n    {\n      title: <span>В&nbsp;каких странах находятся сервера для&nbsp; смены&nbsp;IP?</span>,\n      content: <div>Три страны на выбор: Польша, Германия, Турция.</div>,\n    },\n    // {\n    //   title: <span>Что такое приоритетное подключение?</span>,\n    //   content: (\n    //     <div>\n    //       При подключении смартфона к&nbsp;базовым станциям&nbsp;&mdash; как в&nbsp;какой-то точке в&nbsp;городе, так\n    //       и&nbsp;на&nbsp;транспорте&nbsp;&mdash; всегда действуют настройки приоритизации. У&nbsp;пользователей Membrana в&nbsp;сети самый\n    //       высокий приоритет&nbsp;&mdash; это значит, что они подключаются быстрее и скорость в&nbsp;сети у&nbsp;них более чем\n    //       в&nbsp;2&nbsp;раза выше. Ресурсы базовой станции распределяются между клиентами в&nbsp;зоне её&nbsp;действия: подключаются все,\n    //       просто с&nbsp;разной скоростью.\n    //     </div>\n    //   ),\n    // },\n    {\n      title: <span>Как работает сервис Голос?</span>,\n      content: (\n        <div>\n          <p>\n            Когда вы&nbsp;задали соответствующие настройки или когда просто не&nbsp;берёте трубку, на&nbsp;звонки в&nbsp;Мембране отвечает\n            секретарь. Наш секретарь анализирует входящий вызов и&nbsp;принимает решение: отклонить сразу, поговорить самому\n            и&nbsp;сохранить разговор или соединить с&nbsp;вами. Его задача&nbsp;&mdash; не&nbsp;только отсеять или принять на&nbsp;себя\n            спам, но&nbsp;и&nbsp;передать вам всю существенную информацию. Для этого мы&nbsp;обучаем его на&nbsp;смоделированных\n            и&nbsp;реальных диалогах.\n          </p>\n          <br />\n          <strong>Как секретарь анализирует входящий вызов:</strong>\n          <ol style={{ listStyle: 'disc', margin: '0', padding: '0 0 0 20px' }}>\n            <li>\n              Понимает, есть&nbsp;ли звонящий в&nbsp;контактах, а&nbsp;если нет, то&nbsp;самостоятельно анализирует, может&nbsp;ли владелец\n              этого номера быть вашим знакомым.\n            </li>{' '}\n            <li>Проверяет, есть&nbsp;ли номер в&nbsp;базе известных доверенных организаций или в&nbsp;базе спамеров и&nbsp;мошенников.</li>{' '}\n            <li>Определяет спамеров и&nbsp;мошенников с&nbsp;помощью искусственного интеллекта.</li>{' '}\n            <li>\n              Анализирует контекст разговора, чтобы понять цель звонка. Если действительно важно ответить сейчас&nbsp;&mdash; вы&nbsp;видите\n              вызов, если нет, то&nbsp;секретарь общается сам и&nbsp;всё записывает.\n            </li>\n          </ol>\n          <br />\n          <p>\n            Вы&nbsp;увидите уведомление с&nbsp;превью о&nbsp;новом разговоре и&nbsp;сможете прочитать расшифровку в&nbsp;удобное время.\n            Диалог похож на&nbsp;обычную переписку в&nbsp;мессенджере.\n          </p>\n          <br />\n          <p>\n            Все эти настройки гибкие: если вы&nbsp;захотите, чтобы близкие и&nbsp;коллеги всегда дозванивались до&nbsp;вас\n            немедленно&nbsp;&mdash; так и&nbsp;будет.\n          </p>\n        </div>\n      ),\n    },\n    {\n      title: <span>Сколько дополнительных номеров? Как ими пользоваться?</span>,\n      content: (\n        <div>\n          Вы&nbsp;можете по&nbsp;очереди подключить до&nbsp;3&nbsp;дополнительных номеров. Используйте их&nbsp;для регистрации\n          на&nbsp;различных интернет-ресурсах и&nbsp;в&nbsp;сервисах, чтобы не&nbsp;сообщать свой основной номер и&nbsp;не&nbsp;попадать под\n          рекламный скоринг.\n        </div>\n      ),\n    },\n    {\n      title: 'Для чего нужны профили?',\n      content: (\n        <div>\n          По&nbsp;умолчанию создаются два профиля: &laquo;Общий&raquo; и&nbsp;&laquo;Не&nbsp;беспокоить&raquo;, но&nbsp;вы&nbsp;можете\n          создать любые. Например, &laquo;Дом&raquo;, &laquo;Работа&raquo;, &laquo;Встреча&raquo;, &laquo;Отдых&raquo;. В&nbsp;каждом\n          профиле действуют свои настройки для групп контактов: кто может до&nbsp;вас дозвониться и&nbsp;для кого вы&nbsp;в&nbsp;этом режиме\n          недоступны.\n        </div>\n      ),\n    },\n    {\n      title: 'Как управлять группами контактов?',\n      content: (\n        <div>\n          Для управления входящим голосовым трафиком в&nbsp;приложении Membrana реализован механизм профилей. В&nbsp;настройках профиля все\n          номера сначала разделены на&nbsp;Неизвестные номера и&nbsp;Номера из&nbsp;списка контактов. Для доступа к списку контактов вам\n          необходимо предоставить соответствующее разрешение в&nbsp;приложении. Разрешение можно предоставить еще на&nbsp;этапе регистрации,\n          либо выдать позже при создании группы контактов или напрямую в&nbsp;настройках операционной системы смартфона. Вы&nbsp;можете\n          создать свои группы контактов (например, &laquo;Семья&raquo;, &laquo;Коллеги&raquo;, &laquo;Друзья&raquo;) и&nbsp;назначать\n          им&nbsp;правила:<p> — Напрямую принимать звонки: «На меня».</p> <p>— Отправлять звонки на Секретаря.</p>\n          <p>\n            &mdash;&nbsp;Сбрасывать: &laquo;Блок&raquo;. Вы&nbsp;можете создать профили с&nbsp;разными правилами для одних\n            и&nbsp;тех&nbsp;же групп контактов. Например, настроить профиль &laquo;Дом&raquo; таким образом, чтобы до&nbsp;вас могли\n            дозвониться только друзья и&nbsp;родственники, а&nbsp;профиль &laquo;Работа&raquo;&nbsp;&mdash; чтобы дозвониться могли только\n            коллеги и&nbsp;партнёры.\n          </p>\n        </div>\n      ),\n    },\n    {\n      title: 'Техподдержка',\n      content: (\n        <div>\n          По всем вопросам:{' '}\n          <a style={{ color: 'inherit', textDecoration: 'underline' }} href={'https://futurecrew.ru/'} target={'_blank'} rel='noreferrer'>\n            futurecrew@mts.ru\n          </a>\n        </div>\n      ),\n    },\n  ];\n\n  const [activeItem, setActiveItem] = useState(null);\n  const handleItemClick = (index) => {\n    if (activeItem === index) {\n      setActiveItem(null);\n    } else {\n      setActiveItem(index);\n    }\n  };\n  return (\n    <div className={Styles.Block}>\n      <div className={Styles.HeadBlock}>\n        <div className={classNames(Styles.Title, Styles.TitleOne)}>\n          <h2>Скоро будет ещё больше возможностей</h2>\n        </div>\n        <div className={Styles.Subtitle}>\n          <p>Мы регулярно добавляем новый функционал</p>\n        </div>\n      </div>\n      <div className={Styles.HeadBlock}>\n        <div className={classNames(Styles.Title, Styles.TitleTwo)}>\n          <h2>Как всё устроено</h2>\n        </div>\n      </div>\n      <div className={Styles.Faq}>\n        {SDFag.map((obj, index) => {\n          return (\n            <FagItem\n              isActive={activeItem === index}\n              onItemClick={() => handleItemClick(index)}\n              title={obj.title}\n              content={obj.content}\n              key={index}\n            />\n          );\n        })}\n      </div>\n    </div>\n  );\n};\n\nexport default BlockContent;\n","/Users/troshin/dev/Membrana2/src/components/Research/Research.jsx",["240","241"],[],"import Container from '../ui/Container/Container';\nimport { useWindowSize } from '../../hooks/useWindowSize';\nimport Styles from './Research.module.scss';\n\n\n\nconst Research = () => {\n  const { windowWidth } = useWindowSize();\n  const arrAnimation = [\n    {\n      subTitle: '1 047 044',\n      text: 'вредоносных адреса',\n    },\n    {\n      subTitle: '134 560',\n      text: 'рекламных скриптов',\n    },\n    {\n      subTitle: '55 885',\n      text: 'трекинговых ресурсов',\n    },\n  ];\n\n\n  return (\n   