utton/SoundButton';\nimport FaqBlock from './components/FaqBlock/FaqBlock';\nimport Footer from './components/Footer/Footer';\nimport '@assets/styles/index.scss';\nimport Styles from '@assets/styles/Home.module.scss';\nimport FormBlock from './components/FormBlock/FormBlock';\nimport { useRef } from 'react';\n\nconst blockContentArr = [\n  {\n    title: (\n      <span>\n        Вредоносный <br/>трафик – мимо\n      </span>\n    ),\n    text: [\n      <span>\n        С&nbsp;Membrana сёрфинг в&nbsp;интернете <br/> быстрый и&nbsp;безопасный. На&nbsp;любом сайте мы&nbsp;блокируем<br/> все угрозы и&nbsp;подозрительные скрипты\n      </span>,\n      <span>\n        С&nbsp;Membrana сёрфинг в&nbsp;интернете быстрый<br/> и&nbsp;безопасный. На&nbsp;любом сайте мы&nbsp;блокируем<br/> все угрозы и&nbsp;подозрительные скрипты\n      </span>,\n    ],\n    videoWidth: 494,\n    videoHeight: 440,\n    video: 'https://storage.yandexcloud.net/cdn.tsekh.design/Membrana/benefits/1_Privacy-Secure.mp4#t=0.001',\n    gradient:\n      'conic-gradient(from 254deg at 0.5% 34.76%, #FFF 0deg, rgba(255, 255, 255, 0.90) 36.81592583656311deg, rgba(255, 255, 255, 0.00) 360deg)',\n    id: 1,\n    circlePosition: [-280, 'auto', 'auto', -150],\n    circleSize: [613, 613],\n  },\n  {\n    title: (\n      <span>\n        Спамеры <br/> не пройдут\n      </span>\n    ),\n    text: [\n      <span>\n       Membrana проверяет звонки <br/> от&nbsp;посторонних и&nbsp;блокирует спам.<br/> В&nbsp;приложении можно посмотреть<br/> историю вызовов, на&nbsp;которые ответил<br/> секретарь, и&nbsp;прочитать расшифровку<br/> разговоров\n      </span>,\n      <span>\n       Membrana проверяет звонки от&nbsp;посторонних и&nbsp;блокирует спам.<br/> В&nbsp;приложении можно посмотреть историю вызовов, на&nbsp;которые<br/> ответил секретарь, и&nbsp;прочитать расшифровку разговоров\n      </span>,\n    ],\n    videoWidth: 494,\n    videoHeight: 440,\n    video: 'https://storage.yandexcloud.net/cdn.tsekh.design/Membrana/benefits/2_Prem-Cnnctn.mp4#t=0.001',\n    gradient:\n      'conic-gradient(from 268deg at 0% 34.24%, #FFF 0deg, rgba(255, 255, 255, 0.90) 36.81592583656311deg, rgba(255, 255, 255, 0.00) 360deg)',\n    id: 2,\n    circlePosition: [-300, -300, 'auto', 'auto'],\n    circleSize: [725, 725],\n  },\n  {\n    title: (\n      <span>\n        Реклама исчезает\n      </span>\n    ),\n    text: [\n      <span>\n        Только чистый интернет, только суть.<br/> Без баннеров и&nbsp;навязчивых ненужных <br/> услуг. Выбирайте удобный режим, &laquo;Классический&raquo; или &laquo;Усиленный&raquo;, и&nbsp;наслаждайтесь\n      </span>,\n      <span>\n        Только чистый интернет, только суть. Без баннеров и&nbsp;навязчивых <br/> ненужных услуг. Выбирайте удобный режим, &laquo;Классический&raquo;<br/> или &laquo;Усиленный&raquo;, и&nbsp;наслаждайтесь\n      </span>,\n    ],\n    videoWidth: 398,\n    videoHeight: 398,\n    video: 'https://storage.yandexcloud.net/cdn.tsekh.design/Membrana/benefits/3_Complete-Anonymity.mp4#t=0.001',\n    gradient:\n      'conic-gradient(from -82deg at 0% 59.78%, #FFF 0deg, rgba(255, 255, 255, 0.90) 36.81592583656311deg, rgba(255, 255, 255, 0.00) 360deg)',\n    id: 3,\n    circlePosition: [-150, 'auto', 'auto', -800],\n    circleSize: [900, 900],\n  },\n  {\n    title: <>Секретарь за вас</>,\n    text: [\n      <span>\n       Когда вы&nbsp;заняты, на&nbsp;звонки ответит<br/> виртуальный секретарь<br/> с&nbsp;искусственным интеллектом.<br/> Он&nbsp;понимает содержание разговоров <br/> и&nbsp;соединит с&nbsp;вами, если это срочно\n      </span>,\n      <span>\n        Когда вы&nbsp;заняты, на&nbsp;звонки ответит виртуальный секретарь<br/> с&nbsp;искусственным интеллектом. Он&nbsp;понимает содержание<br/> разговоров и&nbsp;соединит с&nbsp;вами, если это срочно\n      </span>,\n    ],\n    videoWidth: 501,\n    videoHeight: 397,\n    video: 'https://storage.yandexcloud.net/cdn.tsekh.design/Membrana/benefits/4_VPN%20(1).mp4#t=0.001',\n    gradient:\n      'conic-