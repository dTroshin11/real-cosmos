ton from './components/ui/SoundButton/SoundButton';\nimport FaqBlock from './components/FaqBlock/FaqBlock';\nimport Footer from './components/Footer/Footer';\nimport '@assets/styles/index.scss';\nimport Styles from '@assets/styles/Home.module.scss';\nimport FormBlock from './components/FormBlock/FormBlock';\nimport { useRef } from 'react';\n\nconst blockContentArr = [\n  {\n    title: (\n        <span>Информация останется конфиденциальной</span>\n    ),\n    text: [\n      <span>\n        На&nbsp;работе, в&nbsp;отпуске, в&nbsp;командировках наш сервис отследит угрозы и&nbsp;заблокирует подозрительные скрипты\n      </span>,\n      <span>\n        На&nbsp;работе, в&nbsp;отпуске, в&nbsp;командировках наш сервис отследит угрозы и&nbsp;заблокирует подозрительные скрипты\n      </span>,\n    ],\n    videoWidth: 494,\n    videoHeight: 440,\n    video: 'https://storage.yandexcloud.net/cdn.tsekh.design/Membrana/benefits/1_Privacy-Secure.mp4#t=0.001',\n    gradient:\n    'linear-gradient(90deg, #FFF -5.87%, rgba(255, 255, 255, 0.90) 6.23%, rgba(255, 255, 255, 0.00) 112.47%);',\n    id: 1,\n    circlePosition: [-280, 'auto', 'auto', -150],\n    circleSize: [613, 613],\n    styles:[{marginLeft:\"0px\"},{marginLeft: \"160px\"}]\n  },\n  {\n    title: (\n      <span>\n        Спамеры больше не&nbsp;отвлекут от&nbsp;важных дел\n      </span>\n    ),\n    text: [\n      <span>\n       Membrana проверяет звонки и&nbsp;блокирует спам сразу, чтобы вы&nbsp;не&nbsp;тратили на&nbsp;него время. Лишние уведомления вас не&nbsp;побеспокоят\n      </span>,\n      <span>\n       Membrana проверяет звонки и&nbsp;блокирует спам сразу, чтобы вы&nbsp;не&nbsp;тратили на&nbsp;него время. Лишние уведомления вас не&nbsp;побеспокоят\n      </span>,\n    ],\n    videoWidth: 494,\n    videoHeight: 440,\n    video: 'https://storage.yandexcloud.net/cdn.tsekh.design/Membrana/benefits/2_Prem-Cnnctn.mp4#t=0.001',\n    gradient:\n      'linear-gradient(269deg, #FFF 22.74%, rgba(255, 255, 255, 0.90) 35.49%, rgba(255, 255, 255, 0.00) 147.43%);',\n    id: 2,\n    circlePosition: [-300, -300, 'auto', 'auto'],\n    circleSize: [725, 725],\n    styles:[{marginRight:\"0px\"},{marginRight: \"160px\"}]\n  },\n  {\n    title: (\n      <span>\n        Реклама исчезнет по&nbsp;вашему желанию\n      </span>\n    ),\n    text: [\n      <span>\n        У&nbsp;сервиса &laquo;Чистый интернет&raquo; есть два режима: &laquo;Классический&raquo; работает на&nbsp;уровне доменов, а&nbsp;&laquo;Усиленный&raquo;&nbsp;&mdash; анализирует контент страниц. Сёрфинг станет приятным и&nbsp;безопасным\n      </span>,\n      <span>\n        У&nbsp;сервиса &laquo;Чистый интернет&raquo; есть два режима: &laquo;Классический&raquo; работает на&nbsp;уровне доменов, а&nbsp;&laquo;Усиленный&raquo;&nbsp;&mdash; анализирует контент страниц. Сёрфинг станет приятным и&nbsp;безопасным\n      </span>,\n    ],\n    videoWidth: 398,\n    videoHeight: 398,\n    video: 'https://storage.yandexcloud.net/cdn.tsekh.design/Membrana/benefits/3_Complete-Anonymity.mp4#t=0.001',\n    gradient:\n      'linear-gradient(269deg, #FFF 22.74%, rgba(255, 255, 255, 0.90) 35.49%, rgba(255, 255, 255, 0.00) 147.43%);',\n    id: 3,\n    circlePosition: [-150, 'auto', 'auto', -800],\n    circleSize: [900, 900],\n    styles:[{marginRight:\"0px\"},{marginLeft: \"120px\"}]\n  },\n  {\n    title: <>Ответит на&nbsp;звонки за&nbsp;вас</>,\n    text: [\n      <span>\n       Сервис &laquo;Голос&raquo; определит важные звонки. Если вашему близкому важен ваш ответ прямо сейчас, то&nbsp;он&nbsp;сразу&nbsp;же вас соединит. На&nbsp;несрочные звонки&nbsp;&mdash; ответит сам и&nbsp;сохранит для вас расшифровку диалога в&nbsp;приложении\n      </span>,\n      <span>\n       Сервис &laquo;Голос&raquo; определит важные звонки. Если вашему близкому важен ваш ответ прямо сейчас, то&nbsp;он&nbsp;сразу&nbsp;же вас соединит. На&nbsp;несрочные звонки&nbsp;&mdash; ответит сам и&nbsp;сохранит для вас расшифровку диалога в&nbsp;приложении\n      </span>,\n    ],\n   